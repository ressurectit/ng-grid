{
    "name": "@anglr/grid",
    "version": "0.0.0-placeholder",
    "description": "Angular module displaying grid",
    "module": "./dist/es5/grid.js",
    "esm2015": "./dist/es2015/grid.js",
    "typings": "./grid.d.ts",
    "sideEffects": false,
    "scripts": {
        "build": "npm run build:grid && npm run build:extensions && npm run build:scrollmagic && npm run build:material",
        "build:grid": "ngc -p tsconfig.es5.json && ngc -p tsconfig.es2015.json",
        "build:extensions": "ngc -p extensions/tsconfig.es5.json && ngc -p extensions/tsconfig.es2015.json",
        "build:scrollmagic": "ngc -p scrollmagic/tsconfig.es5.json && ngc -p scrollmagic/tsconfig.es2015.json",
        "build:material": "ngc -p material/tsconfig.es5.json && ngc -p material/tsconfig.es2015.json",
        "clean": "rimraf dist tests docs src/**/*.d.ts scrollmagic/**/*.d.ts material/**/*.d.ts extensions/**/*.d.ts */*.d.ts */*.metadata.json ./*.metadata.json ./*.d.ts */dist",
        "clean:duplicates": "rimraf */dist/*/src",
        "clean:deps": "rimraf package-lock.json node_modules",
        "clean:all": "npm run clean && npm run clean:deps",
        "ngc": "ngc",
        "version:placeholder": "replace-in-file /\\\"version\\\":\\s\\\".*?\\\"/g \"\\\"version\\\": \\\"0.0.0-placeholder\\\"\" package.json --isRegex",
        "version:git": "ngv -e \"replace-in-file 0.0.0-placeholder $GIT_VERSION package.json\"",
        "prepublishOnly": "npm run bundle && npm run version:placeholder && npm run version:git",
        "postpublish": "npm run version:placeholder && npm run clean",
        "bundle": "npm run clean && npm run build && npm run clean:duplicates",
        "deploy": "npm publish",
        "deploy:next": "npm publish --tag next",
        "pretest": "webpack",
        "test": "karma start karma.conf.js"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ressurectit/ng-grid.git"
    },
    "keywords": [
        "angular",
        "ng",
        "grid"
    ],
    "author": "kukjevov",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ressurectit/ng-grid/issues"
    },
    "homepage": "https://github.com/ressurectit/ng-grid",
    "peerDependencies": {
        "@angular/core": ">=9.0.0-rc.4",
        "@angular/common": ">=9.0.0-rc.4",
        "@angular/router": ">=9.0.0-rc.4",
        "@angular/cdk": ">=8.0.0",
        "@angular/material": ">=8.0.0",
        "rxjs": ">=6.5.0",
        "@jscrpt/common": ">=1.1.0",
        "@anglr/common": ">=6.4.0",
        "scrollmagic": "^2.0.5"
    },
    "devDependencies": {
        "@angular/core": "^9.0.0-rc.4",
        "@angular/common": "^9.0.0-rc.4",
        "@angular/router": "^9.0.0-rc.4",
        "@angular/animations": "^9.0.0-rc.4",
        "@angular/forms": "^9.0.0-rc.4",
        "@angular/cdk": "^8.0.0",
        "@angular/material": "^8.0.0",
        "rxjs": "^6.5.0",
        "@jscrpt/common": "^1.1.0",
        "@anglr/common": "^6.4.0",
        "zone.js": "^0.8.26",
        "@anglr/types": "1.1.17",
        "scrollmagic": "^2.0.5",
        "@types/jasmine": "~2.8.7",
        "@angular/compiler-cli": "^9.0.0-rc.4",
        "@angular/compiler": "^9.0.0-rc.4",
        "@angular/language-service": "^9.0.0-rc.4",
        "@angular/platform-browser-dynamic": "^9.0.0-rc.4",
        "@angular/platform-browser": "^9.0.0-rc.4",
        "tsickle": "0.35.0",
        "karma-cli": "1.0.1",
        "karma": "3.1.1",
        "karma-jasmine": "1.1.2",
        "karma-phantomjs-launcher": "1.0.4",
        "karma-chrome-launcher": "2.2.0",
        "karma-webpack": "3.0.5",
        "karma-jasmine-html-reporter": "1.4.0",
        "jasmine": "3.3.0",
        "awesome-typescript-loader": "5.2.1",
        "angular2-template-loader": "0.6.2",
        "raw-loader": "1.0.0",
        "sass-loader": "7.1.0",
        "css-loader": "1.0.1",
        "file-loader": "3.0.1",
        "style-loader": "0.23.1",
        "@compodoc/compodoc": "1.1.11",
        "rimraf": "2.6.3",
        "copyfiles": "2.1.0",
        "replace-in-file": "4.1.0",
        "typescript": "3.6.4",
        "webpack": "4.32.2",
        "npm-git-version": "1.1.1",
        "moment": "2.24.0"
    }
}
