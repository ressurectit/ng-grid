{
    "name": "@anglr/grid",
    "version": "5.0.0-beta.24",
    "description": "Angular module displaying grid",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "scripts": {
        "build": "npm run ngc",
        "postbuild": "copyfiles -u 2 dist/src/**/*.* dist && copyfiles -u 2 dist/testing/**/*.* testing && copyfiles -u 2 dist/extensions/**/*.* extensions && rimraf dist/src dist/dist dist/node_modules dist/testing dist/extensions && replace-in-file /\\.\\.\\//g ../dist/ extensions/*.js,extensions/*.ts --isRegex",
        "clean": "rimraf dist testing tests docs extensions",
        "clean:deps": "rimraf package-lock.json node_modules",
        "clean:all": "npm run clean && npm run clean:deps",
        "ngc": "ngc",
        "prepublishOnly": "npm run bundle",
        "postpublish": "npm run clean && npm run docs",
        "republish": "npm publish --force",
        "bundle": "npm run clean && npm run build && copyfiles -u 1 src/**/*.html dist && nnrh *.js -d dist",
        "deploy:stable": "npp -b",
        "deploy:next": "npp -p -b -s \"beta\" -t \"next\"",
        "deploy:stable:local": "npp -b --registry http://local:4873",
        "deploy:next:local": "npp -p -b -s \"beta\" -t \"next\" --registry http://local:4873",
        "docs": "compodoc src -d docs -n '@anglr/grid' -p tsconfig.json --disablePrivate --disableInternal --disableLifeCycleHooks",
        "postdocs": "copyfiles -u 1 docs/**/*.* \\\\DOCS-SERVER\\docs\\ng-grid && npm run clean",
        "pretest": "webpack",
        "test": "karma start karma.conf.js"
    },
    "keywords": [
        "angular",
        "ng",
        "grid"
    ],
    "author": "kukjevov",
    "license": "MIT",
    "peerDependencies": {
        "@angular/core": ">=5.0.0",
        "@angular/common": ">=5.0.0",
        "@angular/router": ">=5.0.0",
        "rxjs": ">=5.5.0",
        "@anglr/common": ">=4.0.0"
    },
    "devDependencies": {
        "@angular/core": "^5.0.0",
        "@angular/common": "^5.0.0",
        "@angular/router": "^5.0.0",
        "@angular/forms": "^5.0.0",
        "rxjs": "^5.5.0",
        "zone.js": "^0.8.26",
        "@types/numeral": "~0.0.22",
        "@anglr/types": "1.1.12",
        "@anglr/common": "^4.0.0",
        "numeral": "2.0.6",
        "@types/node": "~9.6.0",
        "@types/jasmine": "~2.8.6",
        "@angular/compiler-cli": "^5.0.0",
        "@angular/compiler": "^5.0.0",
        "@angular/language-service": "^5.0.0",
        "@angular/platform-browser-dynamic": "^5.0.0",
        "@angular/platform-browser": "^5.0.0",
        "phantomjs-prebuilt": "~2.1.16",
        "karma-cli": "~1.0.1",
        "karma": "~2.0.0",
        "karma-jasmine": "~1.1.1",
        "karma-phantomjs-launcher": "~1.0.4",
        "karma-chrome-launcher": "~2.2.0",
        "karma-webpack": "~3.0.0",
        "karma-jasmine-html-reporter": "~1.0.0",
        "jasmine": "~3.1.0",
        "awesome-typescript-loader": "~3.5.0",
        "angular2-template-loader": "~0.6.2",
        "raw-loader": "~0.5.1",
        "sass-loader": "~6.0.7",
        "css-loader": "~0.28.11",
        "file-loader": "~1.1.11",
        "style-loader": "~0.20.3",
        "@compodoc/compodoc": "~1.0.9",
        "rimraf": "~2.6.2",
        "copyfiles": "~2.0.0",
        "replace-in-file": "3.4.0",
        "typescript": "2.6.2",
        "webpack": "3.8.1",
        "npm-publish-package": "~1.2.0",
        "npm-ng-require-html": "~1.0.0"
    }
}
