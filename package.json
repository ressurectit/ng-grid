{
    "name": "@anglr/grid",
    "version": "0.0.0-placeholder",
    "description": "Angular module displaying grid",
    "type": "module",
    "sideEffects": false,
    "scripts": {
        "build": "ngc --declaration --declarationDir . --declarationMap && ngc -t es2015 --outDir es2015 && npm run dts:rollup",
        "dts:rollup": "npm run api -- api-extractor.json && npm run api -- extensions/api-extractor.json && npm run api -- material/api-extractor.json && npm run api -- scrollmagic/api-extractor.json",
        "api": "api-extractor run --local --verbose -c",
        "clean": "rimraf */temp temp es2015 es2020 **/tsdoc-metadata.json src/**/*.d.ts src/**/*.d.ts.map */src/**/*.d.ts */src/**/*.d.ts.map ./*.d.ts ./*.d.ts.map */*.d.ts",
        "clean:deps": "rimraf package-lock.json node_modules",
        "clean:all": "npm run clean && npm run clean:deps",
        "ngc": "ngc",
        "version:placeholder": "replace-in-file /\\\"version\\\":\\s\\\".*?\\\"/g \"\\\"version\\\": \\\"0.0.0-placeholder\\\"\" package.json --isRegex",
        "version:git": "ngv -e \"replace-in-file 0.0.0-placeholder $GIT_VERSION package.json && echo $GIT_VERSION > version.bak\"",
        "version:build": "ngv -e \"echo $GIT_VERSION > version.bak\"",
        "prepublishOnly": "npm run bundle && npm run version:placeholder && npm run version:git",
        "postpublish": "npm run doc && npm run version:placeholder && npm run clean",
        "bundle": "npm run clean && npm run build",
        "deploy": "npm publish --loglevel error",
        "deploy:next": "npm publish --tag next --loglevel error",
        "lint": "eslint src/**/*.ts",
        "doc": "path-exists docs && npm run doc:generate || echo \"No docs generated!\"",
        "predoc:generate": "cd docs && git pull && cd ..",
        "postdoc:generate": "npm run doc:save",
        "doc:generate": "npm run doc:generate:grid && npm run doc:generate:extensions && npm run doc:generate:scrollmagic && npm run doc:generate:material",
        "doc:save": "cd docs && git add . && git commit -m \"INT: updated API docs ng-grid\" && git push && cd ..",
        "doc:generate:grid": "rimraf docs/content/api/ng-grid && npm run doc:json:grid && npm run doc:md:grid && npm run doc:api:grid",
        "doc:json:grid": "api-extractor run --local --verbose -c api-extractor.json",
        "doc:md:grid": "api-documenter markdown -i temp -o docs/content/api/ng-grid",
        "doc:api:grid": "copyfiles -u 1 temp/grid.api.md docs/content/api/ng-grid && rimraf temp",
        "doc:generate:extensions": "rimraf docs/content/api/ng-grid-extensions && npm run doc:json:extensions && npm run doc:md:extensions && npm run doc:api:extensions",
        "doc:json:extensions": "api-extractor run --local --verbose -c extensions/api-extractor.json",
        "doc:md:extensions": "api-documenter markdown -i extensions/temp -o docs/content/api/ng-grid-extensions",
        "doc:api:extensions": "copyfiles -u 2 extensions/temp/common-extensions.api.md docs/content/api/ng-common-extensions && rimraf extensions/temp",
        "doc:generate:material": "rimraf docs/content/api/ng-grid-material && npm run doc:json:material && npm run doc:md:material && npm run doc:api:material",
        "doc:json:material": "api-extractor run --local --verbose -c material/api-extractor.json",
        "doc:md:material": "api-documenter markdown -i material/temp -o docs/content/api/ng-grid-material",
        "doc:api:material": "copyfiles -u 2 material/temp/grid-material.api.md docs/content/api/ng-grid-material && rimraf material/temp",
        "doc:generate:scrollmagic": "rimraf docs/content/api/ng-grid-scrollmagic && npm run doc:json:scrollmagic && npm run doc:md:scrollmagic && npm run doc:api:scrollmagic",
        "doc:json:scrollmagic": "api-extractor run --local --verbose -c scrollmagic/api-extractor.json",
        "doc:md:scrollmagic": "api-documenter markdown -i scrollmagic/temp -o docs/content/api/ng-grid-scrollmagic",
        "doc:api:scrollmagic": "copyfiles -u 2 scrollmagic/temp/common-scrollmagic.api.md docs/content/api/ng-common-scrollmagic && rimraf scrollmagic/temp"
    },
    "engines": {
        "node": "^12.20.0 || ^14.15.0 || >=16.10.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ressurectit/ng-grid.git"
    },
    "keywords": [
        "angular",
        "ng",
        "grid"
    ],
    "author": "kukjevov",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ressurectit/ng-grid/issues"
    },
    "homepage": "https://github.com/ressurectit/ng-grid",
    "peerDependencies": {
        "@angular/core": ">=13.1.0",
        "@angular/common": ">=13.1.0",
        "@angular/router": ">=13.1.0",
        "@angular/platform-browser": ">=13.1.0",
        "@angular/cdk": ">=13.1.0",
        "@angular/material": ">=13.1.0",
        "rxjs": ">=6.6.7",
        "@jscrpt/common": ">=2.2.0",
        "@anglr/common": ">=10.0.0",
        "scrollmagic": "^2.0.8",
        "tslib": "^2.3.1"
    },
    "devDependencies": {
        "@angular/core": "^13.1.0",
        "@angular/common": "^13.1.0",
        "@angular/router": "^13.1.0",
        "@angular/platform-browser": ">=13.1.0",
        "@angular/forms": "^13.1.0",
        "@angular/cdk": "^13.1.0",
        "@angular/animations": "^13.1.0",
        "@angular/material": "^13.1.0",
        "rxjs": "^6.6.7",
        "@jscrpt/common": "^2.2.0",
        "@anglr/common": "^10.0.0",
        "scrollmagic": "^2.0.8",
        "@angular/compiler-cli": "^13.1.0",
        "@angular/compiler": "^13.1.0",
        "@angular/language-service": "^13.1.0",
        "@microsoft/api-extractor": "7.19.2",
        "@microsoft/api-documenter": "7.13.77",
        "tslib": "2.3.1",
        "eslint": "7.32.0",
        "@typescript-eslint/eslint-plugin": "4.33.0",
        "@typescript-eslint/parser": "4.33.0",
        "eslint-plugin-ressurectit": "0.1.0",
        "rimraf": "3.0.2",
        "copyfiles": "2.4.1",
        "replace-in-file": "6.3.2",
        "path-exists-cli": "2.0.0",
        "typescript": "4.5.4",
        "npm-git-version": "2.0.0"
    },
    "esm2020": "./es2020/src/index.js",
    "es2020": "./es2020/src/index.js",
    "es2015": "./es2015/src/index.js",
    "module": "./es2015/src/index.js",
    "typings": "./grid.d.ts",
    "exports": {
        "./package.json": {
            "default": "./package.json"
        },
        ".": {
            "types": "./grid.d.ts",
            "esm2020": "./es2020/src/index.js",
            "es2020": "./es2020/src/index.js",
            "es2015": "./es2015/src/index.js",
            "node": "./es2015/src/index.js",
            "default": "./es2020/src/index.js"
        },
        "./extensions": {
            "types": "./extensions/grid-extensions.d.ts",
            "esm2020": "./es2020/extensions/src/index.js",
            "es2020": "./es2020/extensions/src/index.js",
            "es2015": "./es2015/extensions/src/index.js",
            "node": "./es2015/extensions/src/index.js",
            "default": "./es2020/extensions/src/index.js"
        },
        "./material": {
            "types": "./material/grid-material.d.ts",
            "esm2020": "./es2020/material/src/index.js",
            "es2020": "./es2020/material/src/index.js",
            "es2015": "./es2015/material/src/index.js",
            "node": "./es2015/material/src/index.js",
            "default": "./es2020/material/src/index.js"
        },
        "./scrollmagic": {
            "types": "./scrollmagic/grid-scrollmagic.d.ts",
            "esm2020": "./es2020/scrollmagic/src/index.js",
            "es2020": "./es2020/scrollmagic/src/index.js",
            "es2015": "./es2015/scrollmagic/src/index.js",
            "node": "./es2015/scrollmagic/src/index.js",
            "default": "./es2020/scrollmagic/src/index.js"
        }
    }
}
